{
  "development": {
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": { "ssl": {
      "rejectUnauthorized": false
    } },
    "dialect": "postgres"
  },
  "test": {
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": { },
    "dialect": "postgres"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialectOptions": { "ssl": true },
    "dialect": "postgres"
  }
}
